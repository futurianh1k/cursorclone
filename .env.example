# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD
POSTGRES_DB=cursor_poc
POSTGRES_PORT=5432

# Redis
REDIS_PORT=6379
REDIS_POOL_SIZE=10

# API Server
API_PORT=8000
JWT_SECRET_KEY=CHANGE_ME_JWT_SECRET_KEY_AT_LEAST_32_CHARS
MASTER_ENCRYPTION_KEY=CHANGE_ME_ENCRYPTION_KEY_GENERATE_WITH_FERNET
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
VLLM_BASE_URL=http://localhost:8001/v1
VLLM_API_KEY=dummy-key
